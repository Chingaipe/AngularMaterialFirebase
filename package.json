{
  "name": "ngxmatfire",
  "version": "1.1.0",
  "author": "Jeroen Ouwehand",
  "description": "Full stack starter app with Angular 6, Material Design and Firebase.",
  "keywords": [
    "angular",
    "angular 6",
    "firebase",
    "material",
    "nosql",
    "oauth",
    "starter-template",
    "ngx"
  ],
  "repository": "https://github.com/jeroenouw/AngularMaterialFirebase",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "postinstall": "concurrently \"webdriver-manager update $CHROMEDRIVER_VERSION_ARG\" \"ngc -p angular.tsconfig.json\"",
    "serve": "ibazel run //src:devserver",
    "pree2e": "bazel build test/...",
    "e2e": "concurrently \"bazel run //src:prodserver\" \"while ! nc -z 127.0.0.1 8080; do sleep 1; done && protractor\" --kill-others --success first",
    "prebuildifier": "bazel build @com_github_bazelbuild_buildtools//buildifier",
    "buildifier": "find . -type f \\( -name BUILD -or -name BUILD.bazel \\) ! -path \"./node_modules/*\" | xargs $(bazel info bazel-bin)/external/com_github_bazelbuild_buildtools/buildifier/*/buildifier"
  },
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/cdk": "^6.0.1",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/material": "^6.0.1",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/platform-server": "^6.0.0",
    "@angular/router": "^6.0.0",
    "core-js": "^2.5.3",
    "firebase": "^5.0.2",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.1.0",
    "rxjs-compat": "^6.1.0",
    "ts-helpers": "*",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/bazel": "^6.0.1",
    "@angular/cli": "^6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@bazel/ibazel": "^0.3.1",
    "@compodoc/compodoc": "^1.1.2",
    "@types/jasmine": "^2.8.7",
    "@types/node": "^10.0.3",
    "codelyzer": "^4.2.1",
    "jasmine-core": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.0.0",
    "protractor": "^5.3.1",
    "rxjs-tslint": "^0.1.3",
    "ts-node": "^6.0.2",
    "tslint": "^5.10.0",
    "typescript": "2.7.2",
    "webpack": "^4.6.0"
  }
}
